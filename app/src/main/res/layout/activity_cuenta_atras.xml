<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"

    android:orientation="vertical"
    tools:context="com.wix.noworneverandroid.padelwear.CuentaAtras">


    <android.support.wearable.view.DelayedConfirmationView
        android:id="@+id/cuenta_atras"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/icon_cancel"
        app:circle_border_color="@color/light_grey"
        app:circle_border_width="10dp"
        app:circle_color="@color/blue"
        app:circle_radius="60dp"></android.support.wearable.view.DelayedConfirmationView>


</LinearLayout>
